{
  "type": "SharedSlice",
  "id": "faq_list",
  "name": "Faq",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "description": "Standard FAQ section displaying a title, a description, and multiple collapsible Q&A pairs.",
      "version": "initial",
      "imageUrl": "",
      "docURL": "...",
      "primary": {
        "faq_title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "",
            "allowTargetBlank": true,
            "multi": "heading1,heading2,strong,em"
          }
        },
        "faq_description": {
          "type": "StructuredText",
          "config": {
            "label": "Description",
            "placeholder": "",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em"
          }
        },
        "faq_items": {
          "type": "Group",
          "config": {
            "label": "FAQ Items",
            "repeat": true,
            "fields": {
              "question": {
                "type": "StructuredText",
                "config": {
                  "label": "Question",
                  "placeholder": "",
                  "allowTargetBlank": true,
                  "single": "paragraph,heading3,heading4,strong,em"
                }
              },
              "answer": {
                "type": "StructuredText",
                "config": {
                  "label": "Answer",
                  "placeholder": "",
                  "allowTargetBlank": true,
                  "multi": "paragraph,strong,em,hyperlink"
                }
              }
            }
          }
        },
        "styles": {
          "type": "Group",
          "config": {
            "label": "Styles",
            "repeat": false,
            "fields": {
              "faq_background_color": {
                "type": "Color",
                "config": {
                  "label": "Background Color",
                  "placeholder": ""
                }
              },
              "faq_title_text_color": {
                "type": "Color",
                "config": {
                  "label": "Title Text Color",
                  "placeholder": ""
                }
              },
              "faq_title_text_size": {
                "type": "Number",
                "config": {
                  "label": "Title Text Size (px)",
                  "placeholder": ""
                }
              },
              "faq_title_text_font": {
                "type": "Select",
                "config": {
                  "label": "Title Text Font",
                  "placeholder": "",
                  "options": [
                    "Inter",
                    "Lora",
                    "Poppins",
                    "Roboto_Mono"
                  ]
                }
              },
              "faq_description_text_color": {
                "type": "Color",
                "config": {
                  "label": "Description Text Color",
                  "placeholder": ""
                }
              },
              "faq_description_text_font": {
                "type": "Select",
                "config": {
                  "label": "Description Text Font",
                  "placeholder": "",
                  "options": [
                    "Inter",
                    "Lora",
                    "Poppins",
                    "Roboto_Mono"
                  ]
                }
              },
              "faq_description_text_size": {
                "type": "Number",
                "config": {
                  "label": "Description Text Size (px)",
                  "placeholder": ""
                }
              },
              "faq_question_text_color": {
                "type": "Color",
                "config": {
                  "label": "Question Text Color",
                  "placeholder": ""
                }
              },
              "faq_question_text_font": {
                "type": "Select",
                "config": {
                  "label": "Question Text Font",
                  "placeholder": "",
                  "options": [
                    "Inter",
                    "Lora",
                    "Poppins",
                    "Roboto_Mono"
                  ]
                }
              },
              "faq_question_text_size": {
                "type": "Number",
                "config": {
                  "label": "Question Text Size (px)",
                  "placeholder": ""
                }
              },
              "faq_answer_text_color": {
                "type": "Color",
                "config": {
                  "label": "Asnwer Text Color",
                  "placeholder": ""
                }
              },
              "faq_answer_text_font": {
                "type": "Select",
                "config": {
                  "label": "Answer Text Font",
                  "placeholder": "",
                  "options": [
                    "Inter",
                    "Lora",
                    "Poppins",
                    "Roboto_Mono"
                  ]
                }
              },
              "faq_answer_text_size": {
                "type": "Number",
                "config": {
                  "label": "Answer Text Size (px)",
                  "placeholder": ""
                }
              },
              "faq_divider_color": {
                "type": "Color",
                "config": {
                  "label": "Divider Color",
                  "placeholder": ""
                }
              }
            }
          }
        }
      },
      "items": {}
    }
  ]
}